<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinApp.AppProperties">
    <TableView Intent="Form">
        <TableRoot>
            <TableSection>
                <EntryCell x:Name="title" Placeholder="Title" Completed="OnChanged"></EntryCell>
                <SwitchCell x:Name="notificationEnabled" Text="Notification" OnChanged="OnChanged"></SwitchCell>
            </TableSection>

        </TableRoot>
    </TableView>
</ContentPage>